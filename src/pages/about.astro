---

// LAYOUTS
import Layout from "../layouts/layout/index.astro"

// COMPONENTS
import Header from "../components/common/header/index.astro"

// ATOMS
import HeadingOne from "../atoms/headings/one/index.astro"

// IMPORT CONTENT
import * as aboutMD from "../content/pages/about.md"
